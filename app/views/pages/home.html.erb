  <body>

    <div class="site-wrapper">

      <div class="site-wrapper-inner">

        <div class="cover-container">

          <div class="masthead clearfix">
            <div class="inner">
              <h3 class="masthead-brand cover-heading">SynergySpace</h3>
              <nav>
                <ul class="nav masthead-nav">
                  <li class="active"><a href="/">Home</a></li>
                </ul>
              </nav>
            </div>
          </div>

          <div class="inner cover">
            <h1 class="cover-heading">SynergySpace</h1>
            <p class="lead">Sugar, space, and everything nice: coworking space rental and teaming to succeed service. Sign up to lease or rent the perfect office space with office mates that match your energy.</p>
            <p class="lead">
              <%= link_to "Sign up", signup_path, class: "btn btn-lg btn-default" %>
            </p>
          </div>

          <div class="mastfoot">
            <div class="inner">
              <p>by Blue Screen (Alexei Darmin, Ben Fridman, Mark Kazakevich, Theresa Ma)</p>
            </div>
          </div>

        </div>

      </div>

    </div>
  </body>
